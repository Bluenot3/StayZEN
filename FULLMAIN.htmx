        #canvas-container {
            width: 100%;
            max-width: 1400px;
            padding: 20px;
            box-sizing: border-box;
            position: relative;
            z-index: 1;
        }

        #animation-container {
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            z-index: 0;
        }

        .embedded-bot-container {
            width: 100%;
            height: 300px;
            margin-bottom: 40px;
            display: flex;
            align-items: center;
            justify-content: center;
            position: relative;
            overflow: hidden;
            transition: height 0.5s cubic-bezier(0.68, -0.55, 0.265, 1.55);
            cursor: pointer;
            background: rgba(255, 255, 255, 0.05);
            border-radius: 20px;
            backdrop-filter: blur(10px);
            box-shadow: 0 0 20px rgba(0, 255, 255, 0.2);
        }

        .embedded-bot-container.expanded {
            height: 700px;
        }

        .expand-button {
            position: absolute;
            bottom: 10px;
            right: 10px;
            background: rgba(0, 255, 255, 0.2);
            border: none;
            color: white;
            padding: 8px 15px;
            border-radius: 30px;
            cursor: pointer;
            transition: all 0.3s ease;
            font-family: 'Orbitron', sans-serif;
            text-transform: uppercase;
            letter-spacing: 1px;
        }

        .expand-button:hover {
            background: rgba(0, 255, 255, 0.4);
            box-shadow: var(--neon-glow);
        }

        .game-interface {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
            gap: 30px;
        }

        .game-tile {
            background: rgba(255, 255, 255, 0.05);
            border-radius: 20px;
            padding: 30px;
            text-align: center;
            cursor: pointer;
            transition: all 0.5s cubic-bezier(0.25, 0.8, 0.25, 1);
            text-decoration: none;
            color: #fff;
            position: relative;
            overflow: hidden;
            backdrop-filter: blur(5px);
            box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
        }

        .game-tile::before {
            content: '';
            position: absolute;
            top: -50%;
            left: -50%;
            width: 200%;
            height: 200%;
            background: radial-gradient(circle, rgba(0,255,255,0.2) 0%, rgba(0,255,255,0) 70%);
            opacity: 0;
            transition: opacity 0.5s ease, transform 0.5s ease;
            transform: scale(0.1);
        }

        .game-tile:hover::before {
            opacity: 1;
            transform: scale(1);
        }

        .game-tile:hover {
            transform: translateY(-10px) scale(1.05);
            box-shadow: 0 15px 30px rgba(0, 255, 255, 0.4);
            background: rgba(255, 255, 255, 0.1);
        }

        .game-icon {
            font-size: 54px;
            margin-bottom: 20px;
            transition: transform 0.5s cubic-bezier(0.68, -0.55, 0.265, 1.55);
            text-shadow: var(--neon-glow);
        }

        .game-tile:hover .game-icon {
            transform: scale(1.2) rotate(15deg);
        }

        .game-text {
            font-size: 18px;
            opacity: 0.8;
            transition: all 0.5s ease;
            font-weight: 400;
            text-transform: uppercase;
            letter-spacing: 1px;
        }

        .game-tile:hover .game-text {
            opacity: 1;
            transform: scale(1.1);
            text-shadow: var(--neon-glow);
        }

        @media (max-width: 768px) {
            #canvas-container {
                padding: 10px;
            }

            .embedded-bot-container {
                height: 250px;
            }

            .embedded-bot-container.expanded {
                height: 550px;
            }

            .game-interface {
                grid-template-columns: repeat(2, 1fr);
            }
        }

        @media (max-width: 480px) {
            .game-interface {
                grid-template-columns: 1fr;
            }

            .embedded-bot-container {
                height: 200px;
            }

            .embedded-bot-container.expanded {
                height: 450px;
            }
        }
    </style>
</head>
<body>
